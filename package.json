{
  "name": "Mnemnk",
  "description": "Browser extension for Mnemnk to record your web browsing history",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.4",
    "@types/chrome": "^0.0.280",
    "@wxt-dev/auto-icons": "^1.0.2",
    "@wxt-dev/module-svelte": "^2.0.0",
    "svelte": "^5.1.16",
    "svelte-check": "^4.0.7",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "wxt": "^0.19.13"
  },
  "dependencies": {
    "@reactivex/rxjs": "^6.6.7",
    "rxjs": "^7.8.1"
  }
}
